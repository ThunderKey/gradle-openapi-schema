    return onUserDto.apply(asUserDto());
  }
  else if ("NNVariantsValue".equals(discriminator) && isValidAgainstNecessityAndNullability()) {
    return onNecessityAndNullabilityDto.apply(asNecessityAndNullabilityDto());
